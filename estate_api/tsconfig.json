{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
	"moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "types": ["node"],
    "outDir": "dist",
    "module": "nodenext",
    "target": "ESNext"
  }
}
